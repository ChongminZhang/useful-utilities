\bar under
1D elements
\end_layout

\begin_layout Standard
We interpolate the functions with Lagrange polynomials of degree 
\begin_inset Formula $N$
\end_inset

 with GLL points 
\begin_inset Formula $(\xi_{\alpha})_{\alpha=0\ldots N}$
\end_inset

 as collocation points.
 For any function 
\begin_inset Formula $f:\xi\mapsto f(\xi)$
\end_inset

 on 
\begin_inset Formula $\Lambda=[-1,1]$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\forall\xi\in[-1,1]\quad f(\boldsymbol{x}(\xi))=\sum_{\alpha=0}^{N}f(\xi_{\alpha})\ell_{\alpha}(\xi)=\sum_{\alpha=0}^{N}f^{\alpha}\ell_{\alpha}(\xi)
\]

\end_inset


\end_layout

\begin_layout Standard
and :
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\forall\xi\in[-1,1]\quad\dfrac{\partial f}{\partial\xi}(\boldsymbol{x}(\xi))=\sum_{\alpha=0}^{N}f(\xi_{\alpha})\dfrac{d\ell_{\alpha}}{d\xi}(\xi)=\sum_{\alpha=0}^{N}f^{\alpha}\ell'_{\alpha}(\xi)
\]

\end_inset


\end_layout

\begin_layout Standard
The 
\begin_inset Formula $\ell_{\alpha}$
\end_inset

 are the Lagrange polynomials defined on the colocation points of the Gauss-Loba
tto-Legendre quadrature.
 The 
\begin_inset Formula $\xi_{\alpha}$
\end_inset

 are the GLL points.
 It is of paramount importance to note that 
\begin_inset Formula $\ell_{i}(\xi_{j})=\delta_{ij}$
\end_inset

.
\end_layout

\begin_layout Standard
We evaluate the integrals with the quadrature :
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
{\displaystyle \int_{-1}^{1}f(\xi)\mathrm{d}\xi\approx\sum_{i=0}^{N}\omega_{i}f^{i}}
\]

\end_inset


\end_layout

\begin_layout Itemize

\series bold
\bar under
Surface elements
\end_layout

\begin_layout Standard
We interpolate the functions with Lagrange polynomials of degree 
\begin_inset Formula $N$
\end_inset

 with GLL points 
\begin_inset Formula $(\xi_{\alpha},\eta_{\beta})_{\alpha,\beta=0\ldots N}$
\end_inset

 as collocation points (here we should add more detail for a paper).
 For any function 
\begin_inset Formula $f:(x,z)\mapsto f(x,z)$
\end_inset

 on 
\begin_inset Formula $\Omega_{e}$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\forall(\xi,\eta)\in\Lambda\quad f(\boldsymbol{x}(\xi,\eta))\approx\sum_{\alpha=0}^{N}\sum_{\beta=0}^{N}f(\xi_{\alpha},\eta_{\beta})\ell_{\alpha}(\xi)\ell_{\beta}(\eta)=\sum_{\alpha,\beta=0}^{N}f^{\alpha\beta}\ell_{\alpha}(\xi)\ell_{\beta}(\eta)
\]

\end_inset


\end_layout

\begin_layout Standard
and :
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{array}{ccccccc}
\forall(\xi,\eta)\in\Lambda & \quad\\
 &  & \dfrac{\partial f}{\partial\xi}(\boldsymbol{x}(\xi,\eta)) & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}f^{\alpha\beta}\dfrac{\partial\ell_{\alpha}(\xi)}{\partial\xi}\ell_{\beta}(\eta)} & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}f^{\alpha\beta}\ell'_{\alpha}(\xi)\ell_{\beta}(\eta)}\\
 &  & \dfrac{\partial f}{\partial\eta}(\boldsymbol{x}(\xi,\eta)) & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}f^{\alpha\beta}\ell_{\alpha}(\xi)\dfrac{\partial\ell_{\beta}(\eta)}{\partial\eta}} & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}f^{\alpha\beta}\ell_{\alpha}(\xi)\ell'_{\beta}(\eta)}
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
The 
\begin_inset Formula $\ell_{i}$
\end_inset

 are the Lagrange polynomials defined on the colocation points of the Gauss-Loba
tto-Legendre quadrature.
 The 
\begin_inset Formula $\xi_{i},\eta_{i}$
\end_inset

 are the GLL points.
 It is of paramount importance to note that 
\begin_inset Formula $\ell_{i}(\xi_{j})=\delta_{ij}$
\end_inset

.
\end_layout

\begin_layout Standard
We evaluate the integrals with the quadrature :
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\int_{\Lambda}f(\boldsymbol{x}(\xi,\eta))\:\mathrm{d\xi}\mathrm{d\eta}\approx\sum_{i,j=0}^{N}\omega_{i}\omega_{j}f^{ij}
\]

\end_inset


\end_layout

\begin_layout Subsection*
Derivation of the algebraic system
\end_layout

\begin_layout Standard
\noindent
We begin by calculating the potential gradient on the GLL points 
\begin_inset Formula $(\xi_{\sigma},\eta_{\nu})_{\sigma,\nu=0\ldots N}$
\end_inset

 of element 
\begin_inset Formula $\Omega_{e}$
\end_inset

:
\end_layout

\begin_layout Standard
\noindent
\begin_inset Formula 
\[
\begin{array}{ccl}
\partial_{i}\chi & = & \dfrac{\partial\chi}{\partial\xi}\partial_{i}\xi+\dfrac{\partial\chi}{\partial\eta}\partial_{i}\eta\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}\chi^{\alpha\beta}\ell'_{\alpha}(\xi_{\sigma})\ell_{\beta}(\eta_{\nu})}\partial_{i}\xi+{\displaystyle \sum_{\alpha,\beta=0}^{N}\chi^{\alpha\beta}\ell_{\alpha}(\xi_{\sigma})\ell'_{\beta}(\eta_{\nu})}\partial_{i}\eta\\
 & = & {\displaystyle \sum_{\alpha=0}^{N}\chi^{\alpha\nu}\ell'_{\alpha}(\xi_{\text{\sigma}})}\partial_{i}\xi+{\displaystyle \sum_{\beta=0}^{N}\chi^{\sigma\beta}\ell'_{\beta}(\eta_{\nu})}\partial_{i}\eta\\
 & = & \left[{\displaystyle \sum_{\alpha=0}^{N}\chi^{\alpha\nu}\ell'_{\alpha}(\xi_{\text{\sigma}})}\right]\partial_{i}\xi+\left[{\displaystyle \sum_{\alpha=0}^{N}\chi^{\sigma\alpha}\ell'_{\alpha}(\eta_{\nu})}\right]\partial_{i}\eta
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
We need the four terms 
\begin_inset Formula $\partial_{x}\xi$
\end_inset

,
\begin_inset Formula $\partial_{z}\xi$
\end_inset

,
\begin_inset Formula $\partial_{x}\eta$
\end_inset

,
\begin_inset Formula $\partial_{z}\eta$
\end_inset

.
 
\end_layout

\begin_layout Standard
The GLL interpolation tells :
\begin_inset Formula 
\[
\begin{array}{ccccc}
\dfrac{\partial w}{\partial\xi}(\xi_{\sigma},\eta_{\nu}) & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}\ell'_{\alpha}(\xi_{\sigma})\ell{}_{\beta}(\eta_{\nu})} & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}\ell'_{\alpha}(\xi_{\sigma})\delta_{\beta\nu}}\\
\dfrac{\partial w}{\partial\eta}(\xi_{\sigma},\eta_{\nu}) & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}\ell_{\alpha}(\xi_{\sigma})\ell'_{\beta}(\eta_{\nu})} & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}\delta_{\alpha\sigma}\ell'_{\beta}(\eta_{\nu})}
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
We use then the quadrature rule to calculate the elemental stiffness integral
 :
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{array}{ccl}
{\displaystyle \int_{\Lambda}\dfrac{1}{\rho}\boldsymbol{\nabla}w\cdot\boldsymbol{\nabla}\chi\:\left|\mathcal{J}_{e}\right|\mathrm{d\xi}\mathrm{d\eta}} & = & {\displaystyle \int_{\Lambda}\dfrac{1}{\rho}\left[\dfrac{\partial w}{\partial\xi}\left(\partial_{x}\xi\dfrac{\partial\chi}{\partial x}+\partial_{z}\xi\dfrac{\partial\chi}{\partial z}\right)+\dfrac{\partial w}{\partial\eta}\left(\partial_{x}\eta\dfrac{\partial\chi}{\partial x}+\partial_{z}\eta\dfrac{\partial\chi}{\partial z}\right)\right]\:\left|\mathcal{J}_{e}\right|\mathrm{d\xi}\mathrm{d\eta}}\\
 & = & {\displaystyle \sum_{\sigma,\nu=0}^{N}\omega_{\sigma}\omega_{\nu}\dfrac{\left|\mathcal{J}_{e}^{\sigma\nu}\right|}{\rho^{\sigma\nu}}\left[\sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}\ell'_{\alpha}(\xi_{\sigma})\delta_{\beta\nu}\left(\partial_{x}\xi\dfrac{\partial\chi}{\partial x}+\partial_{z}\xi\dfrac{\partial\chi}{\partial z}\right)+\sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}\delta_{\alpha\sigma}\ell'_{\beta}(\eta_{\nu})\left(\partial_{x}\eta\dfrac{\partial\chi}{\partial x}+\partial_{z}\eta\dfrac{\partial\chi}{\partial z}\right)\right]}\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}\left[\omega_{\beta}\sum_{\sigma=0}^{N}\omega_{\sigma}\dfrac{\left|\mathcal{J}_{e}^{\sigma\beta}\right|}{\rho^{\sigma\beta}}\ell'_{\alpha}(\xi_{\sigma})\left(\partial_{x}\xi\dfrac{\partial\chi}{\partial x}+\partial_{z}\xi\dfrac{\partial\chi}{\partial z}\right)+\omega_{\alpha}\sum_{\nu=0}^{N}\omega_{\nu}\dfrac{\left|\mathcal{J}_{e}^{\alpha\nu}\right|}{\rho^{\alpha\nu}}\ell'_{\beta}(\eta_{\nu})\left(\partial_{x}\eta\dfrac{\partial\chi}{\partial x}+\partial_{z}\eta\dfrac{\partial\chi}{\partial z}\right)\right]}\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}B^{\alpha\beta}}
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
We follow the same reasoning for the elemental mass integral :
\begin_inset Formula 
\[
\begin{array}{cll}
{\displaystyle \int_{\Lambda}w\dfrac{1}{\lambda}\ddot{\chi}\:\left|\mathcal{J}_{e}\right|\mathrm{d\xi}\mathrm{d\eta}} & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}\omega_{\alpha}\omega_{\beta}\dfrac{1}{\lambda^{\alpha\beta}}\left|\mathcal{J}_{e}^{\alpha\beta}\right|\ddot{\chi}^{\alpha\beta}}\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}A^{\alpha\beta}}
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
The coupling integral reads : 
\begin_inset Formula 
\[
\begin{array}{ccc}
\int_{-1}^{1}w\boldsymbol{n}\cdot\dot{\boldsymbol{u}}\:\dfrac{dx}{d\xi}\mathrm{d\xi} & = & {\displaystyle \sum_{\alpha=0}^{N}w^{\alpha N}\omega_{\alpha}\left.\dfrac{dx}{d\xi}\right|^{\alpha}\left(n_{x}^{\alpha}\dot{u}_{x}^{\alpha}+n_{z}^{\alpha}\dot{u}_{z}^{\alpha}\right)}\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}\omega_{\alpha}\left.\dfrac{dx}{d\xi}\right|^{\alpha}\left(n_{x}^{\alpha}\dot{u}_{x}^{\alpha}+n_{z}^{\alpha}\dot{u}_{z}^{\alpha}\right)\delta_{\beta N}}\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}C^{\alpha\beta}}
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
As the relation : 
\begin_inset Formula ${\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}A^{\alpha\beta}}+{\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}B^{\alpha\beta}}=C^{\alpha\beta}$
\end_inset

 must hold for any test function 
\begin_inset Formula $w(x,z)$
\end_inset

, we can conclude :
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\forall\alpha,\beta=0\dots N\quad\forall e=0\dots n_{e}-1\quad A^{\alpha\beta}+B^{\alpha\beta}=0\quad\left(w=\delta_{\alpha\beta}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
We have then :
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{array}{c}
\forall\alpha,\beta=0\dots N\quad\forall e=0\dots n_{e}-1\quad\\
\begin{array}{rll}
\omega_{\alpha}\omega_{\beta}\dfrac{1}{\lambda^{\alpha\beta}}\left|\mathcal{J}_{e}^{\alpha\beta}\right|\ddot{\chi}^{\alpha\beta}+{\displaystyle \left[\omega_{\beta}\sum_{\sigma=0}^{N}\omega_{\sigma}\dfrac{\left|\mathcal{J}_{e}^{\sigma\beta}\right|}{\rho^{\sigma\beta}}\ell'_{\alpha}(\xi_{\sigma})\left(\partial_{x}\xi\dfrac{\partial\chi}{\partial x}+\partial_{z}\xi\dfrac{\partial\chi}{\partial z}\right)+\omega_{\alpha}\sum_{\nu=0}^{N}\omega_{\nu}\dfrac{\left|\mathcal{J}_{e}^{\alpha\nu}\right|}{\rho^{\alpha\nu}}\ell'_{\beta}(\eta_{\nu})\left(\partial_{x}\eta\dfrac{\partial\chi}{\partial x}+\partial_{z}\eta\dfrac{\partial\chi}{\partial z}\right)\right]} & = & \omega_{\alpha}\left.\dfrac{dx}{d\xi}\right|^{\alpha}\left(n_{x}^{\alpha}\dot{u}_{x}^{\alpha}+n_{z}^{\alpha}\dot{u}_{z}^{\alpha}\right)\delta_{\beta N}\\
M^{\alpha\beta}\ddot{\chi}^{\alpha\beta}+{\displaystyle \left[\sum_{I,J=0}^{N}K{}^{\alpha\beta IJ}\chi^{IJ}\right]} & = & C^{\alpha\beta}
\end{array}
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
Or in tensorial form for each element 
\begin_inset Formula $e$
\end_inset

 :
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
{\displaystyle \boldsymbol{M}\odot\ddot{\boldsymbol{\chi}}+\boldsymbol{K}\cdot\boldsymbol{\chi}}=\boldsymbol{0}
\]

\end_inset


\end_layout

\begin_layout Standard
(We have defined 
\begin_inset Formula $\left(\boldsymbol{A}\odot\boldsymbol{B}\right)_{ij}=A_{ij}B_{ij}$
\end_inset

 and 
\begin_inset Formula ${\displaystyle \left(\boldsymbol{A}\cdot\boldsymbol{B}\right)_{ij}=\sum_{k,l=0}^{N}A{}_{ijkl}B_{kl}}$
\end_inset

 ).
 Hence, after assembly :
\begin_inset Formula 
\[
{\displaystyle \ddot{\boldsymbol{\chi}}}=\dfrac{\boldsymbol{F}_{x,int}^{g}(t)}{\boldsymbol{M}^{g}}
\]

\end_inset


\end_layout

\begin_layout Section*
2.5D spectral-elements formulation in curvilinear cylindrical coordinates
 (acoustic medium)
\end_layout

\begin_layout Subsection*
Introduction : assumptions, conventions, weak form
\end_layout

\begin_layout Standard
We shall start from a 3D formulation.
 I recall the weak form of the 3D momentum equation : 
\end_layout

\begin_layout Standard
\noindent
\begin_inset Formula 
\[
\begin{array}{ccl}
\underset{\text{ mass integral}}{\underbrace{{\displaystyle \int_{\Omega}w\dfrac{1}{\lambda}\ddot{\chi}\:\mathrm{d^{3}\boldsymbol{x}}}}} & = & -\underset{\text{ stiffness integral}}{\underbrace{\int_{\Omega}\dfrac{1}{\rho}\boldsymbol{\nabla}w\cdot\boldsymbol{\nabla}\chi\:\mathrm{d^{3}\boldsymbol{x}}}}+\underset{\text{coupling integral}}{\underbrace{\int_{\Gamma}w\boldsymbol{n}\cdot\dot{\boldsymbol{u}}\:\mathrm{dx}}}\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
The test functions : 
\begin_inset Formula $\left(r,\theta,z\right)\mapsto w(r,\theta,z)$
\end_inset

 now belongs to the subspace of Sobolev space 
\begin_inset Formula $H_{1}^{1}(\Omega)$
\end_inset

 of the functions that cancel on the axis.
 (Bernardi and p.12 of Alexandre Fournier phd tesis, we would have to introduce
 these spaces).
 The wave equation reads :
\end_layout

\begin_layout Standard
\noindent
\begin_inset Formula 
\begin{equation}
\begin{array}{clll}
 & {\displaystyle \int_{\Omega}w\dfrac{1}{\lambda}\ddot{\chi}\:\mathrm{d^{3}\boldsymbol{x}}} & = & {\displaystyle -\int_{\Omega}\dfrac{1}{\rho}\boldsymbol{\nabla}w\cdot\boldsymbol{\nabla}\chi\:\mathrm{d^{3}\boldsymbol{x}}+\int_{\Gamma}w\boldsymbol{n}\cdot\dot{\boldsymbol{u}}\:2\pi r\mathrm{dr}}\\
\Longleftrightarrow & {\displaystyle \int_{\Omega}w\dfrac{1}{\lambda}\ddot{\chi}\:2\pi r\mathrm{dr}\mathrm{d\theta}\mathrm{dz}} & = & -{\displaystyle \int_{\Omega}\dfrac{1}{\rho}\boldsymbol{\nabla}w\cdot\boldsymbol{\nabla}\chi\:2\pi r\mathrm{dr}\mathrm{d\theta}\mathrm{dz}}+\int_{\Gamma}w\boldsymbol{n}\cdot\dot{\boldsymbol{u}}\:2\pi r\mathrm{dx}
\end{array}\label{eq:weakForm2.5D}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
We still consider an 
\color blue
acoustic isotropic media,
\color inherit
 the stress tensor still expresses : 
\begin_inset Formula 
\[
\forall i,j\in\{x,y,z\}^{2}\quad T_{ij}=\lambda\delta_{ij}\epsilon_{kk}=\lambda\delta_{ij}(\epsilon_{11}+\epsilon_{22}+\epsilon_{33})
\]

\end_inset

 Nevertheless now 
\begin_inset Formula $\boldsymbol{\nabla}f=\left(\begin{array}{c}
\dfrac{\partial f}{\partial r}\\
\dfrac{1}{r}\dfrac{\partial f}{\partial\theta}\\
\dfrac{\partial f}{\partial z}
\end{array}\right)$
\end_inset

 consequently : 
\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none

\begin_inset Formula $\boldsymbol{\nabla}w\cdot\boldsymbol{\nabla}\chi=\dfrac{\partial w}{\partial r}\dfrac{\partial\chi}{\partial r}+\dfrac{1}{r^{2}}\dfrac{\partial w}{\partial\theta}\dfrac{\partial\chi}{\partial\theta}+\dfrac{\partial w}{\partial z}\dfrac{\partial\chi}{\partial z}$
\end_inset

.
 Moreover 
\begin_inset Formula $\left\{ \begin{array}{ccc}
u_{r} & = & \dfrac{1}{\rho}\dfrac{\partial\chi}{\partial r}\\
u_{\theta} & = & \dfrac{1}{\rho r}\dfrac{\partial\chi}{\partial\theta}\\
u_{z} & = & \dfrac{1}{\rho}\dfrac{\partial\chi}{\partial z}
\end{array}\right.$
\end_inset


\end_layout

\begin_layout Standard
At this point we will choose the 
\color blue
2.5D convention : we suppose an axisymetric geometry and that the important
 loads are not along 
\begin_inset Formula $\boldsymbol{\theta}$
\end_inset

 and do not change with 
\begin_inset Formula $\theta$
\end_inset


\color inherit
:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\dfrac{\partial}{\partial\theta}=0
\]

\end_inset


\end_layout

\begin_layout Standard
That leads to : 
\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\strikeout off
\uuline off
\uwave off
\noun off
\color none

\begin_inset Formula $\boldsymbol{\nabla}w\cdot\boldsymbol{\nabla}\chi=\dfrac{\partial w}{\partial r}\dfrac{\partial\chi}{\partial r}+\dfrac{\partial w}{\partial z}\dfrac{\partial\chi}{\partial z}$
\end_inset

 and 
\begin_inset Formula $\left\{ \begin{array}{ccc}
u_{r} & = & \dfrac{1}{\rho}\dfrac{\partial\chi}{\partial r}\\
u_{\theta} & = & 0\\
u_{z} & = & \dfrac{1}{\rho}\dfrac{\partial\chi}{\partial z}
\end{array}\right.$
\end_inset

which is exactly the same than in cartesian coordinates with 
\begin_inset Formula $r\longleftrightarrow x$
\end_inset

.
\end_layout

\begin_layout Subsection*
For non-axial elements
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{array}{ccl}
{\displaystyle \int_{\Lambda}\dfrac{1}{\rho}\boldsymbol{\nabla}w\cdot\boldsymbol{\nabla}\chi\:\left|\mathcal{J}_{e}\right|r\mathrm{d\xi}\mathrm{d\eta}} & = & {\displaystyle \int_{\Lambda}\dfrac{1}{\rho}\left[\dfrac{\partial w}{\partial\xi}\left(\partial_{r}\xi\dfrac{\partial\chi}{\partial r}+\partial_{z}\xi\dfrac{\partial\chi}{\partial z}\right)+\dfrac{\partial w}{\partial\eta}\left(\partial_{r}\eta\dfrac{\partial\chi}{\partial r}+\partial_{z}\eta\dfrac{\partial\chi}{\partial z}\right)\right]\:\left|\mathcal{J}_{e}\right|r\mathrm{d\xi}\mathrm{d\eta}}\\
 & = & {\displaystyle \sum_{\sigma,\nu=0}^{N}\omega_{\sigma}\omega_{\nu}\dfrac{\left|\mathcal{J}_{e}^{\sigma\nu}\right|r^{\sigma\nu}}{\rho^{\sigma\nu}}\left[\sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}\ell'_{\alpha}(\xi_{\sigma})\delta_{\beta\nu}\left(\partial_{r}\xi\dfrac{\partial\chi}{\partial r}+\partial_{z}\xi\dfrac{\partial\chi}{\partial z}\right)+\sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}\delta_{\alpha\sigma}\ell'_{\beta}(\eta_{\nu})\left(\partial_{r}\eta\dfrac{\partial\chi}{\partial r}+\partial_{z}\eta\dfrac{\partial\chi}{\partial z}\right)\right]}\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}\left[\omega_{\beta}\sum_{\sigma=0}^{N}\omega_{\sigma}\dfrac{\left|\mathcal{J}_{e}^{\sigma\beta}\right|}{\rho^{\sigma\beta}}{\color{green}r^{\sigma\beta}}\ell'_{\alpha}(\xi_{\sigma})\left(\partial_{r}\xi\dfrac{\partial\chi}{\partial r}+\partial_{z}\xi\dfrac{\partial\chi}{\partial z}\right)+\omega_{\alpha}\sum_{\nu=0}^{N}\omega_{\nu}\dfrac{\left|\mathcal{J}_{e}^{\alpha\nu}\right|}{\rho^{\alpha\nu}}{\color{green}r^{\alpha\nu}}\ell'_{\beta}(\eta_{\nu})\left(\partial_{r}\eta\dfrac{\partial\chi}{\partial r}+\partial_{z}\eta\dfrac{\partial\chi}{\partial z}\right)\right]}\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}B^{\alpha\beta}}
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{array}{cll}
{\displaystyle \int_{\Lambda}w\dfrac{1}{\lambda}\ddot{\chi}\:\left|\mathcal{J}_{e}\right|r\mathrm{d\xi}\mathrm{d\eta}} & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}\omega_{\alpha}\omega_{\beta}\dfrac{1}{\lambda^{\alpha\beta}}\left|\mathcal{J}_{e}^{\alpha\beta}\right|{\color{green}r^{\alpha\beta}}\ddot{\chi}^{\alpha\beta}}\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}A^{\alpha\beta}}
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{array}{ccc}
\int_{-1}^{1}w\boldsymbol{n}\cdot\dot{\boldsymbol{u}}\:\dfrac{dr}{d\xi}r\mathrm{d\xi} & = & {\displaystyle \sum_{\alpha=0}^{N}w^{\alpha N}\omega_{\alpha}\left.\dfrac{dr}{d\xi}\right|^{\alpha}r^{\alpha}\left(n_{r}^{\alpha}\dot{u}_{r}^{\alpha}+n_{z}^{\alpha}\dot{u}_{z}^{\alpha}\right)}\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}\omega_{\alpha}\left.\dfrac{dx}{d\xi}\right|^{\alpha}{\color{green}r^{\alpha}}\left(n_{r}^{\alpha}\dot{u}_{r}^{\alpha}+n_{z}^{\alpha}\dot{u}_{z}^{\alpha}\right)\delta_{\beta N}}\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\alpha\beta}C^{\alpha\beta}}
\end{array}
\]

\end_inset


\end_layout

\begin_layout Subsection*
For axial elements
\end_layout

\begin_layout Standard
\noindent
\begin_inset Formula 
\[
\begin{array}{ccl}
\partial_{i}\chi(\boldsymbol{r}(\overline{\xi}_{\sigma},\eta_{\nu}),t) & = & \dfrac{\partial\chi}{\partial\xi}\partial_{i}\xi+\dfrac{\partial\chi}{\partial\eta}\partial_{i}\eta\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}\chi^{\overline{\alpha}\beta}\overline{\ell}'_{\alpha}(\overline{\xi}_{\sigma})\ell_{\beta}(\eta_{\nu})}\partial_{i}\xi+{\displaystyle \sum_{\alpha,\beta=0}^{N}\chi^{\overline{\alpha}\beta}\ell_{\alpha}(\overline{\xi}_{\sigma})\ell'_{\beta}(\eta_{\nu})}\partial_{i}\eta\\
 & = & {\displaystyle \sum_{\alpha=0}^{N}\chi^{\overline{\alpha}\nu}\overline{\ell}'_{\alpha}(\overline{\xi}_{\text{\sigma}})}\partial_{i}\xi+{\displaystyle \sum_{\beta=0}^{N}\chi^{\overline{\sigma}\beta}\ell'_{\beta}(\eta)}\partial_{i}\eta\\
 & = & \left[{\displaystyle \sum_{\alpha=0}^{N}\chi^{\overline{\alpha}\nu}\overline{\ell}'_{\alpha}(\overline{\xi}_{\text{\sigma}})}\right]\partial_{i}\xi+\left[{\displaystyle \sum_{\alpha=0}^{N}\chi^{\overline{\sigma}\alpha}\ell'_{\alpha}(\eta)}\right]\partial_{i}\eta
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{array}{ccl}
{\displaystyle \int_{\Lambda}\dfrac{1}{\rho}\boldsymbol{\nabla}w\cdot\boldsymbol{\nabla}\chi\:\left|\mathcal{J}_{e}\right|r\mathrm{d\xi}\mathrm{d\eta}} & = & {\displaystyle \int_{\Lambda}\dfrac{1}{\rho}\left[\dfrac{\partial w}{\partial\xi}\left(\partial_{r}\xi\dfrac{\partial\chi}{\partial r}+\partial_{z}\xi\dfrac{\partial\chi}{\partial z}\right)+\dfrac{\partial w}{\partial\eta}\left(\partial_{r}\eta\dfrac{\partial\chi}{\partial r}+\partial_{z}\eta\dfrac{\partial\chi}{\partial z}\right)\right]\:\left|\mathcal{J}_{e}\right|r\mathrm{d\xi}\mathrm{d\eta}}\\
 & = & {\displaystyle \sum_{\sigma,\nu=0}^{N}\overline{\omega}_{\sigma}\omega_{\nu}\dfrac{\left|\mathcal{J}_{e}^{\overline{\sigma}\nu}\right|}{\rho^{\overline{\sigma}\nu}}\dfrac{r^{\overline{\sigma}\nu}}{1+\overline{\xi}_{\sigma}}\left[\sum_{\alpha,\beta=0}^{N}w^{\overline{\alpha}\beta}\overline{\ell}'_{\alpha}(\overline{\xi}_{\sigma})\delta_{\beta\nu}\left(\partial_{r}\xi\dfrac{\partial\chi}{\partial r}+\partial_{z}\xi\dfrac{\partial\chi}{\partial z}\right)+\sum_{\alpha,\beta=0}^{N}w^{\overline{\alpha}\beta}\delta_{\alpha\sigma}\ell'_{\beta}(\eta_{\nu})\left(\partial_{r}\eta\dfrac{\partial\chi}{\partial r}+\partial_{z}\eta\dfrac{\partial\chi}{\partial z}\right)\right]}\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w^{\overline{\alpha}\beta}\left[\omega_{\beta}\sum_{\sigma=0}^{N}\overline{\omega}_{\sigma}\dfrac{\left|\mathcal{J}_{e}^{\overline{\sigma}\beta}\right|}{\rho^{\overline{\sigma}\beta}}{\color{green}\dfrac{r^{\overline{\sigma}\beta}}{1+\overline{\xi}_{\sigma}}}\overline{\ell}'_{\alpha}(\overline{\xi}_{\sigma})\left(\partial_{r}\xi\dfrac{\partial\chi}{\partial r}+\partial_{z}\xi\dfrac{\partial\chi}{\partial z}\right)+\overline{\omega}_{\alpha}\sum_{\nu=0}^{N}\omega_{\nu}\dfrac{\left|\mathcal{J}_{e}^{\overline{\alpha}\nu}\right|}{\rho^{\overline{\alpha}\nu}}{\color{green}\dfrac{r^{\overline{\alpha}\nu}}{1+\overline{\xi}_{\alpha}}}\ell'_{\beta}(\eta_{\nu})\left(\partial_{r}\eta\dfrac{\partial\chi}{\partial r}+\partial_{z}\eta\dfrac{\partial\chi}{\partial z}\right)\right]}\\
