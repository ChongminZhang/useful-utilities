\begin_layout Subsection*
Derivation of the algebraic system for non-axial elements
\end_layout

\begin_layout Standard
\noindent
We begin by calculating the displacement gradients on the GLL points 
\begin_inset Formula $(\xi_{\sigma},\eta_{\nu})_{\sigma,\nu=0\ldots N}$
\end_inset

 of element 
\begin_inset Formula $\Omega_{e}$
\end_inset

:
\begin_inset Formula 
\[
\begin{array}{ccl}
\partial_{i}u_{j}(\boldsymbol{r}(\xi_{\sigma},\eta_{\nu}),t) & = & \dfrac{\partial u_{j}}{\partial\xi}\partial_{i}\xi+\dfrac{\partial u_{j}}{\partial\eta}\partial_{i}\eta\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}u_{j}^{\alpha\beta}\ell'_{\alpha}(\xi_{\sigma})\ell_{\beta}(\eta_{\nu})}\partial_{i}\xi+{\displaystyle \sum_{\alpha,\beta=0}^{N}u_{j}^{\alpha\beta}\ell_{\alpha}(\xi_{\sigma})\ell'_{\beta}(\eta_{\nu})}\partial_{i}\eta\\
 & = & {\displaystyle \sum_{\alpha=0}^{N}u_{j}^{\alpha\nu}\ell'_{\alpha}(\xi_{\text{\sigma}})}\partial_{i}\xi+{\displaystyle \sum_{\beta=0}^{N}u_{j}^{\sigma\beta}\ell'_{\beta}(\eta_{\nu})}\partial_{i}\eta\\
 & = & \left[{\displaystyle \sum_{\alpha=0}^{N}u_{j}^{\alpha\nu}\ell'_{\alpha}(\xi_{\text{\sigma}})}\right]\partial_{i}\xi+\left[{\displaystyle \sum_{\alpha=0}^{N}u_{j}^{\sigma\alpha}\ell'_{\alpha}(\eta_{\nu})}\right]\partial_{i}\eta
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
\noindent
We need the four terms 
\begin_inset Formula $\partial_{r}\xi$
\end_inset

,
\begin_inset Formula $\partial_{z}\xi$
\end_inset

,
\begin_inset Formula $\partial_{r}\eta$
\end_inset

,
\begin_inset Formula $\partial_{z}\eta$
\end_inset

.
 Then we calculate the five elements of the stress tensor on these GLL points
 :
\begin_inset Formula 
\[
\begin{array}{ccl}
T_{rr} & = & \left(\lambda+2\mu\right)\dfrac{\partial u_{r}}{\partial r}+\lambda\dfrac{\partial u_{z}}{\partial z}{\color{green}+\lambda\dfrac{1}{r}u_{r}}\\
T_{zz} & = & \lambda\dfrac{\partial u_{r}}{\partial r}+\left(\lambda+2\mu\right)\dfrac{\partial u_{z}}{\partial z}{\color{green}+\lambda\dfrac{1}{r}u_{r}}\\
T_{rz} & = & \mu\left[\dfrac{\partial u_{r}}{\partial z}+\dfrac{\partial u_{z}}{\partial r}\right]\\
T_{zr} & = & T_{xz}\\
{\color{green}T_{\theta\theta}} & {\color{green}=} & {\color{green}\lambda\dfrac{\partial u_{r}}{\partial r}+\left(\lambda+2\mu\right)\dfrac{1}{r}u_{r}+\lambda\dfrac{\partial u_{z}}{\partial z}}
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
We calculate the four matrix elements on the GLL points 
\begin_inset Formula ${\displaystyle F_{ik}=\sum_{j=1}^{2}T_{ij}\partial_{j}\xi_{k}}$
\end_inset

.
 With these notations the elemental stiffness integral reads :
\begin_inset Formula 
\[
\begin{array}{ccc}
{\displaystyle \int_{\Lambda}\sum_{i,j=1}^{2}T_{ij}\boldsymbol{\nabla w}_{ji}\:\left|\mathcal{J}_{e}\right|r\mathrm{d\xi}\mathrm{d\eta}} & = & \int_{\Lambda}\left({\displaystyle \sum_{i,k=1}^{2}F_{ik}}\dfrac{\partial w_{i}}{\partial\xi_{k}}+T_{\theta\theta}\dfrac{w_{r}}{r}\right)\:\left|\mathcal{J}_{e}\right|r\mathrm{d\xi}\mathrm{d\eta}\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
The GLL interpolation tells :
\begin_inset Formula 
\[
\begin{array}{ccccccc}
\dfrac{\partial w_{i}}{\partial\xi_{1}}(\xi_{\sigma},\eta_{\nu}) & = & \dfrac{\partial w_{i}}{\partial\xi}(\xi_{\sigma},\eta_{\nu}) & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w_{i}^{\alpha\beta}\ell'_{\alpha}(\xi_{\sigma})\ell{}_{\beta}(\eta_{\nu})} & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w_{i}^{\alpha\beta}\ell'_{\alpha}(\xi_{\sigma})\delta_{\beta\nu}}\\
\dfrac{\partial w_{i}}{\partial\xi_{2}}(\xi_{\sigma},\eta_{\nu}) & = & \dfrac{\partial w_{i}}{\partial\eta}(\xi_{\sigma},\eta_{\nu}) & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w_{i}^{\alpha\beta}\ell_{\alpha}(\xi_{\sigma})\ell'_{\beta}(\eta_{\nu})} & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w_{i}^{\alpha\beta}\delta_{\alpha\sigma}\ell'_{\beta}(\eta_{\nu})}
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
(and of course : 
\begin_inset Formula $w_{r}(\xi_{\sigma},\eta_{\nu})\equiv w_{r}^{\sigma\nu}={\displaystyle \sum_{\alpha,\beta=0}^{N}w_{r}^{\alpha\beta}\delta_{\alpha\sigma}\delta_{\beta\nu}}$
\end_inset

)
\end_layout

\begin_layout Standard
We use then the quadrature rule to calculate the elemental stiffness integral
 :
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{array}{ccl}
{\displaystyle \int_{\Lambda}\sum_{i,j=1}^{2}T_{ij}\boldsymbol{\nabla w}_{ji}\:\left|\mathcal{J}_{e}\right|r\mathrm{d\xi}\mathrm{d\eta}} & = & \int_{\Lambda}\left({\displaystyle \sum_{i,k=1}^{2}F_{ik}}\dfrac{\partial w_{i}}{\partial\xi_{k}}+T_{\theta\theta}\dfrac{w_{r}}{r}\right)\:\left|\mathcal{J}_{e}\right|r\mathrm{d\xi}\mathrm{d\eta}\\
 & = & \int_{\Lambda}\left|\mathcal{J}_{e}\right|\left(r{\displaystyle \sum_{i,k=1}^{2}F_{ik}}\dfrac{\partial w_{i}}{\partial\xi_{k}}+T_{\theta\theta}w_{r}\right)\:\mathrm{d\xi}\mathrm{d\eta}\\
 & = & {\displaystyle \sum_{\sigma,\nu=0}^{N}\omega_{\sigma}\omega_{\nu}\left|\mathcal{J}_{e}^{\sigma\nu}\right|\left[r^{\sigma\nu}\sum_{i=1}^{2}\left(F_{i1}^{\sigma\nu}\sum_{\alpha,\beta=0}^{N}w_{i}^{\alpha\beta}\ell'_{\alpha}(\xi_{\sigma})\delta_{\beta\nu}+F_{i2}^{\sigma\nu}\sum_{\alpha,\beta=0}^{N}w_{i}^{\alpha\beta}\delta_{\alpha\sigma}\ell'_{\beta}(\eta_{\nu})\right)+T_{\theta\theta}^{\sigma\nu}{\displaystyle \sum_{\alpha,\beta=0}^{N}w_{r}^{\alpha\beta}\delta_{\alpha\sigma}\delta_{\beta\nu}}\right]}\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w_{r}^{\alpha\beta}\left[\omega_{\beta}\sum_{\sigma=0}^{N}\omega_{\sigma}\left|\mathcal{J}_{e}^{\sigma\beta}\right|{\color{green}r^{\sigma\beta}}F_{11}^{\sigma\beta}\ell'_{\alpha}(\xi_{\sigma})+\omega_{\alpha}\sum_{\nu=0}^{N}\omega_{\nu}\left|\mathcal{J}_{e}^{\alpha\nu}\right|{\color{green}r^{\alpha\nu}}F_{12}^{\alpha\nu}\ell'_{\beta}(\eta_{\nu})+{\color{green}\omega_{\alpha}\omega_{\beta}\left|\mathcal{J}_{e}^{\alpha\beta}\right|T_{\theta\theta}^{\alpha\beta}}\right]}\\
 &  & +{\displaystyle \sum_{\alpha,\beta=0}^{N}w_{z}^{\alpha\beta}\left[\omega_{\beta}\sum_{\sigma=0}^{N}\omega_{\sigma}\left|\mathcal{J}_{e}^{\sigma\beta}\right|{\color{green}r^{\sigma\beta}}F_{21}^{\sigma\beta}\ell'_{\alpha}(\xi_{\sigma})+\omega_{\alpha}\sum_{\nu=0}^{N}\omega_{\nu}\left|\mathcal{J}_{e}^{\alpha\nu}\right|{\color{green}r^{\alpha\nu}}F_{22}^{\alpha\nu}\ell'_{\beta}(\eta_{\nu})\right]}\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w_{x}^{\alpha\beta}B_{x}^{\alpha\beta}+\sum_{\alpha,\beta=0}^{N}w_{z}^{\alpha\beta}B_{z}^{\alpha\beta}}
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
We follow the same reasoning for the mass integral :
\begin_inset Formula 
\[
\begin{array}{ccl}
{\displaystyle \int_{\Lambda}\boldsymbol{w}\cdot\rho\ddot{\boldsymbol{u}}\:\left|\mathcal{J}_{e}\right|r\mathrm{d\xi}\mathrm{d\eta}} & = & {\displaystyle \sum_{\sigma,\beta=0}^{N}\rho^{\alpha\beta}\left(w_{r}^{\alpha\beta}\ddot{u}_{r}^{\alpha\beta}+w_{z}^{\alpha\beta}\ddot{u}_{z}^{\alpha\beta}\right)r_{e}^{\alpha\beta}\left|\mathcal{J}_{e}^{\alpha\beta}\right|\omega_{\alpha}\omega_{\beta}}\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w_{r}^{\alpha\beta}\omega_{\alpha}\omega_{\beta}\rho^{\alpha\beta}{\color{green}r_{e}^{\alpha\beta}}\left|\mathcal{J}_{e}^{\alpha\beta}\right|\ddot{u}_{r}^{\alpha\beta}}+{\displaystyle \sum_{\alpha,\beta=0}^{N}w_{z}^{\alpha\beta}\omega_{\alpha}\omega_{\beta}\rho^{\alpha\beta}{\color{green}r_{e}^{\alpha\beta}}\left|\mathcal{J}_{e}^{\alpha\beta}\right|\ddot{u}_{z}^{\alpha\beta}}\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w_{r}^{\alpha\beta}A_{r}^{\alpha\beta}}+{\displaystyle \sum_{\alpha,\beta=0}^{N}w_{z}^{\alpha\beta}A_{z}^{\alpha\beta}}
\end{array}
\]

\end_inset


\end_layout

\begin_layout Standard
Moreover the source integral is zero
