\begin_layout Standard
\begin_inset Formula 
\[
\begin{array}{ccl}
{\displaystyle \int_{\Lambda}\sum_{i,j=1}^{2}T_{ij}\boldsymbol{\nabla w}_{ji}\:\left|\mathcal{J}_{e}\right|r\mathrm{d\xi}\mathrm{d\eta}} & = & \int_{\Lambda}\left({\displaystyle \sum_{i,k=1}^{2}F_{ik}}\dfrac{\partial w_{i}}{\partial\xi_{k}}+T_{\theta\theta}\dfrac{w_{r}}{r}\right)\:\left|\mathcal{J}_{e}\right|r\mathrm{d\xi}\mathrm{d\eta}\\
 & = & {\displaystyle \sum_{\sigma,\nu=0}^{N}\overline{\omega}_{\sigma}\omega_{\nu}\left|\mathcal{J}_{e}^{\overline{\sigma}\nu}\right|R^{\overline{\sigma}\nu}\left[\sum_{i=1}^{2}\left(F_{i1}^{\overline{\sigma}\nu}\sum_{\alpha,\beta=0}^{N}w_{i}^{\overline{\alpha}\beta}\overline{\ell}'_{\alpha}(\overline{\xi}_{\sigma})\delta_{\beta\nu}+F_{i2}^{\overline{\sigma}\nu}\sum_{\alpha,\beta=0}^{N}w_{i}^{\overline{\alpha}\beta}\delta_{\alpha\sigma}\ell'_{\beta}(\eta_{\nu})\right)+T_{\theta\theta}^{\overline{\sigma}\nu}\sum_{\alpha,\beta=0}^{N}w_{r}^{\overline{\alpha}\beta}A^{\alpha\beta\nu\sigma}\right]}\\
 & = & {\displaystyle \sum_{i=1}^{2}\sum_{\alpha,\beta=0}^{N}w_{i}^{\overline{\alpha}\beta}\left[\omega_{\beta}\sum_{\sigma=0}^{N}\overline{\omega}_{\sigma}\left|\mathcal{J}_{e}^{\overline{\sigma}\beta}\right|R^{\overline{\sigma}\beta}F_{i1}^{\overline{\sigma}\beta}\overline{\ell}'_{\alpha}(\overline{\xi}_{\sigma})+\overline{\omega}_{\alpha}\sum_{\nu=0}^{N}\omega_{\nu}\left|\mathcal{J}_{e}^{\overline{\alpha}\nu}\right|R^{\overline{\alpha}\nu}F_{i2}^{\overline{\alpha}\nu}\ell'_{\beta}(\eta_{\nu})-\delta_{i1}\sum_{\sigma,\nu=0}^{N}\omega_{\nu}\overline{\omega}_{\sigma}\left|\mathcal{J}_{e}^{\overline{\sigma}\nu}\right|R^{\overline{\sigma}\nu}T_{\theta\theta}^{\overline{\sigma}\nu}A^{\alpha\beta\nu\sigma}\right]}\\
 & = & {\displaystyle \sum_{i=1}^{2}\sum_{\alpha,\beta=0}^{N}w_{i}^{\overline{\alpha}\beta}\left[\omega_{\beta}\sum_{\sigma=0}^{N}\overline{\omega}_{\sigma}\left|\mathcal{J}_{e}^{\overline{\sigma}\beta}\right|R^{\overline{\sigma}\beta}F_{i1}^{\overline{\sigma}\beta}\overline{\ell}'_{\alpha}(\overline{\xi}_{\sigma})+\overline{\omega}_{\alpha}\sum_{\nu=0}^{N}\omega_{\nu}\left|\mathcal{J}_{e}^{\overline{\alpha}\nu}\right|R^{\overline{\alpha}\nu}F_{i2}^{\overline{\alpha}\nu}\ell'_{\beta}(\eta_{\nu})\right.}\\
 &  & {\displaystyle \left.+\delta_{i1}\left(\sum_{\nu=0}^{N}\omega_{\nu}\overline{\omega}_{0}\left|\mathcal{J}_{e}^{\overline{0}\nu}\right|R^{\overline{0}\nu}T_{\theta\theta}^{\overline{0}\nu}A^{\alpha\beta\nu0}+\sum_{\nu=0}^{N}\sum_{\sigma=1}^{N}\omega_{\nu}\overline{\omega}_{\sigma}\left|\mathcal{J}_{e}^{\overline{\sigma}\nu}\right|R^{\overline{\sigma}\nu}T_{\theta\theta}^{\overline{\sigma}\nu}A^{\alpha\beta\nu\sigma}\right)\right]}\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w_{r}^{\overline{\alpha}\beta}\left[\omega_{\beta}\sum_{\sigma=0}^{N}\overline{\omega}_{\sigma}\left|\mathcal{J}_{e}^{\overline{\sigma}\beta}\right|R^{\overline{\sigma}\beta}F_{11}^{\overline{\sigma}\beta}\ell'_{\alpha}(\overline{\xi}_{\sigma})+\overline{\omega}_{\alpha}\sum_{\nu=0}^{N}\omega_{\nu}\left|\mathcal{J}_{e}^{\overline{\alpha}\nu}\right|R^{\overline{\alpha}\nu}F_{12}^{\overline{\alpha}\nu}\ell'_{\beta}(\eta_{\nu})\right.}\\
 &  & {\displaystyle \left.+\omega_{\beta}\left(\overline{\omega}_{0}\left|\mathcal{J}_{e}^{\overline{0}\beta}\right|R^{\overline{0}\beta}T_{\theta\theta}^{\overline{0}\beta}\left(\dfrac{\partial r}{\partial\xi}\right)^{-1}\overline{\ell}'_{\alpha}(\overline{\xi}_{0})+\sum_{\sigma=1}^{N}\overline{\omega}_{\sigma}\left|\mathcal{J}_{e}^{\overline{\sigma}\beta}\right|R^{\overline{\sigma}\beta}T_{\theta\theta}^{\overline{\sigma}\beta}\dfrac{1}{r^{\overline{\sigma}\beta}}\delta_{\alpha\sigma}\right)\right]}\\
 &  & {\displaystyle +\sum_{\alpha,\beta=0}^{N}w_{z}^{\overline{\alpha}\beta}\left[\omega_{\beta}\sum_{\sigma=0}^{N}\overline{\omega}_{\sigma}\left|\mathcal{J}_{e}^{\overline{\sigma}\beta}\right|R^{\overline{\sigma}\beta}F_{21}^{\overline{\sigma}\beta}\overline{\ell}'_{\alpha}(\overline{\xi}_{\sigma})+\overline{\omega}_{\alpha}\sum_{\nu=0}^{N}\omega_{\nu}\left|\mathcal{J}_{e}^{\overline{\alpha}\nu}\right|R^{\overline{\alpha}\nu}F_{22}^{\overline{\alpha}\nu}\ell'_{\beta}(\eta_{\nu})\right]}\\
 & = & {\displaystyle \sum_{\alpha,\beta=0}^{N}w_{r}^{\overline{\alpha}\beta}\left[\vphantom{1cm}\omega_{\beta}\sum_{\sigma=0}^{N}\overline{\omega}_{\sigma}\left|\mathcal{J}_{e}^{\overline{\sigma}\beta}\right|{\color{green}R^{\overline{\sigma}\beta}}F_{11}^{\overline{\sigma}\beta}\overline{\ell}'_{\alpha}(\overline{\xi}_{\sigma})+\overline{\omega}_{\alpha}\sum_{\nu=0}^{N}\omega_{\nu}\left|\mathcal{J}_{e}^{\overline{\alpha}\nu}\right|{\color{green}R^{\overline{\alpha}\nu}}F_{12}^{\overline{\alpha}\nu}\ell'_{\beta}(\eta_{\nu})\right.}\\
 &  & {\displaystyle \left.{\color{green}+\omega_{\beta}\left(\overline{\omega}_{0}\left|\mathcal{J}_{e}^{\overline{0}\beta}\right|T_{\theta\theta}^{\overline{0}\beta}\overline{\ell}'_{\alpha}(\overline{\xi}_{0})+\begin{cases}
