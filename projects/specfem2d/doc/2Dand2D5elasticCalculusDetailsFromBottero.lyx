\begin_layout Subsection*
Mapping
\end_layout

\begin_layout Standard
We subdivide the model volume 
\begin_inset Formula $\Omega$
\end_inset

 into a number of non-overlapping hexahedral elements 
\begin_inset Formula $\Omega_{e}$
\end_inset

 , 
\begin_inset Formula $e=1,\ldots,n_{h}\times n_{v}$
\end_inset

, such that 
\begin_inset Formula ${\displaystyle \Omega=\bigcup_{e}\Omega_{e}}$
\end_inset

.
 As the result of this subdivision, the artificial boundary 
\begin_inset Formula $\Gamma$
\end_inset

 would be similarly represented by a number of 1D elements 
\begin_inset Formula $\Gamma_{b}$
\end_inset

, 
\begin_inset Formula $b=1,\ldots,n_{b}$
\end_inset

 such that 
\begin_inset Formula ${\displaystyle \Gamma=\bigcup_{b}\Gamma_{b}}$
\end_inset

 but we don't consider it for now.
 The 
\begin_inset Formula $n_{v}$
\end_inset

 2D elements along the axis need to be distinguished.
 
\end_layout

\begin_layout Itemize

\series bold
\bar under
Surface elements (for the moment we just have surface elements)
\end_layout

\begin_layout Standard
Points 
\begin_inset Formula $\boldsymbol{r}=\left(r,z\right)$
\end_inset

 within each hexahedral non-axial element 
\begin_inset Formula $\Omega_{e}$
\end_inset

 may be uniquely related to points 
\begin_inset Formula $\boldsymbol{\xi}=(\xi,\eta)$
\end_inset

, 
\begin_inset Formula $-1\leq\xi,\eta\leq1$
\end_inset

 in a reference square 
\begin_inset Formula $\Lambda$
\end_inset

 based upon the invertible mapping 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\boldsymbol{r}(\boldsymbol{\xi})=\sum_{a=1}^{n_{a}}\boldsymbol{r}_{a}N_{a}(\boldsymbol{\xi})
\]

\end_inset

The 
\begin_inset Formula $a=1,\ldots,n_{a}$
\end_inset

 anchors 
\begin_inset Formula $\boldsymbol{r}_{a}=\boldsymbol{r}_{e}(\xi_{a},\eta_{a})$
\end_inset

 and shape functions 
\begin_inset Formula $N_{a}(\boldsymbol{\xi})$
\end_inset

 define the geometry of an element 
\begin_inset Formula $\Omega_{e}.$
\end_inset

 A surface element 
\begin_inset Formula $\mathrm{d^{2}\boldsymbol{x}}=2\pi r\mathrm{dr}\mathrm{dz}$
\end_inset

 within a given element 
\begin_inset Formula $\Omega_{e}$
\end_inset

 is related to a surface element in the reference square by the relation
 : 
\begin_inset Formula 
\[
2\pi r\mathrm{dr}\mathrm{dz}=2\pi r(\xi,\eta)\left|\mathcal{J}_{e}\right|\mathrm{d\xi}\mathrm{d\eta}
\]

\end_inset


\end_layout

\begin_layout Standard
Where 
\begin_inset Formula $J_{e}$
\end_inset

 is the surface Jacobian.
 For 
\begin_inset Formula $(r,z)\in\Omega_{e}$
\end_inset

 it expresses :
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathcal{J}_{e}=\left|\dfrac{\partial r}{\partial\xi}\dfrac{\partial z}{\partial\eta}-\dfrac{\partial r}{\partial\eta}\dfrac{\partial z}{\partial\xi}\right|
\]

\end_inset


\end_layout

\begin_layout Standard
With of course 
\begin_inset Formula ${\displaystyle \dfrac{\partial r}{\partial\xi}=\sum_{a=1}^{n_{a}}\dfrac{\partial N_{a}}{\partial\xi}r_{a}}$
\end_inset

, 
\begin_inset Formula ${\displaystyle \dfrac{\partial z}{\partial\xi}=\sum_{a=1}^{n_{a}}\dfrac{\partial N_{a}}{\partial\xi}z_{a}}$
\end_inset

, 
\begin_inset Formula ${\displaystyle \dfrac{\partial r}{\partial\eta}=\sum_{a=1}^{n_{a}}\dfrac{\partial N_{a}}{\partial\eta}r_{a}}$
\end_inset

 and 
\begin_inset Formula ${\displaystyle \dfrac{\partial z}{\partial\eta}=\sum_{a=1}^{n_{a}}\dfrac{\partial N_{a}}{\partial\eta}z_{a}}$
\end_inset

.
 Moreover for any function 
\begin_inset Formula $(r,z)\in\Omega_{e}\mapsto f(r,z)$
\end_inset

 we have the three identities :
\end_layout

\begin_layout Standard
\begin_inset Formula $\left(\begin{array}{c}
\dfrac{\partial f}{\partial\xi}\\
\dfrac{\partial f}{\partial\eta}
\end{array}\right)=\underset{J_{e}}{\underbrace{\left(\begin{array}{cc}
\dfrac{\partial r}{\partial\xi} & \dfrac{\partial z}{\partial\xi}\\
\dfrac{\partial r}{\partial\eta} & \dfrac{\partial z}{\partial\eta}
\end{array}\right)}}\times\left(\begin{array}{c}
\dfrac{\partial f}{\partial r}\\
\dfrac{\partial f}{\partial z}
\end{array}\right)=\left(\begin{array}{c}
\dfrac{\partial f}{\partial r}\dfrac{\partial r}{\partial\xi}+\dfrac{\partial f}{\partial z}\dfrac{\partial z}{\partial\xi}\\
\dfrac{\partial f}{\partial r}\dfrac{\partial r}{\partial\eta}+\dfrac{\partial f}{\partial z}\dfrac{\partial z}{\partial\eta}
\end{array}\right)$
\end_inset

 ,
\begin_inset Formula $\left(\begin{array}{c}
\dfrac{\partial f}{\partial r}\\
\dfrac{\partial f}{\partial z}
\end{array}\right)=\underset{J_{e}^{-1}}{\underbrace{\left(\begin{array}{cc}
\dfrac{\partial\xi}{\partial r} & \dfrac{\partial\eta}{\partial r}\\
\dfrac{\partial\xi}{\partial z} & \dfrac{\partial\eta}{\partial z}
\end{array}\right)}}\times\left(\begin{array}{c}
\dfrac{\partial f}{\partial\xi}\\
\dfrac{\partial f}{\partial\eta}
\end{array}\right)=\left(\begin{array}{c}
\dfrac{\partial f}{\partial\xi}\dfrac{\partial\xi}{\partial r}+\dfrac{\partial f}{\partial\eta}\dfrac{\partial\eta}{\partial r}\\
\dfrac{\partial f}{\partial\xi}\dfrac{\partial\xi}{\partial z}+\dfrac{\partial f}{\partial\eta}\dfrac{\partial\eta}{\partial z}
\end{array}\right)$
\end_inset

 
\end_layout

\begin_layout Standard
and 
\begin_inset Formula $\left(\begin{array}{c}
\dfrac{\partial f}{\partial r}\\
\dfrac{\partial f}{\partial z}
\end{array}\right)=\underset{J_{e}^{-1}}{\underbrace{\dfrac{1}{\mathcal{J}_{e}}\left(\begin{array}{cc}
\dfrac{\partial z}{\partial\eta} & -\dfrac{\partial z}{\partial\xi}\\
-\dfrac{\partial r}{\partial\eta} & \dfrac{\partial r}{\partial\xi}
\end{array}\right)}}\times\left(\begin{array}{c}
\dfrac{\partial f}{\partial\xi}\\
\dfrac{\partial f}{\partial\eta}
\end{array}\right)=\dfrac{1}{\mathcal{J}_{e}}\left(\begin{array}{c}
\dfrac{\partial f}{\partial\xi}\dfrac{\partial z}{\partial\eta}-\dfrac{\partial f}{\partial\eta}\dfrac{\partial z}{\partial\xi}\\
-\dfrac{\partial f}{\partial\xi}\dfrac{\partial r}{\partial\eta}+\dfrac{\partial f}{\partial\eta}\dfrac{\partial r}{\partial\xi}
\end{array}\right)$
\end_inset

 
\end_layout

